int	subMenu_confederaciones (void)
{
	int retono = -1;
	int opcion ;


	do {

		switch(opcion)
		{
		case 1:
			break;
		case 2:
			break;
		case 3:
			break;
		case 4:
			break;
		case 5:
			break;
		case 6:
			break;

		}


	} while (opcion);



	return retono;
}


